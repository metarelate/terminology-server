###  http://metarelate.net/config/PragmaConfigProperties

<http://metarelate.net/config/PragmaConfigProperties> rdf:type owl:ObjectProperty .



###  http://metarelate.net/config/ValidationConfigProperties

<http://metarelate.net/config/ValidationConfigProperties> rdf:type owl:ObjectProperty .



###  http://metarelate.net/config/appliesTo

<http://metarelate.net/config/appliesTo> rdf:type owl:ObjectProperty ;
                                         
                                         rdfs:comment "Specifies to which Terminology Entity (e.r. reigister) a constraint applies. Constraints are transitively propagated to contained entities, unless a clear command is encountered." ;
                                         
                                         rdfs:range <http://metarelate.net/config/TerminologyEntity> ;
                                         
                                         rdfs:subPropertyOf <http://metarelate.net/config/ValidationConfigProperties> ;
                                         
                                         rdfs:domain <http://metarelate.net/config/ValidationTypes> .



###  http://metarelate.net/config/authConfigProperty

<http://metarelate.net/config/authConfigProperty> rdf:type owl:ObjectProperty .



###  http://metarelate.net/config/generatesProperty

<http://metarelate.net/config/generatesProperty> rdf:type owl:ObjectProperty ;
                                                 
                                                 rdfs:comment """<x> generatesProperty <y> implies that for any satement:
a <x> b, a new statement a <y> p is generated.""" ;
                                                 
                                                 rdfs:subPropertyOf <http://metarelate.net/config/reasonerConfigProperty> .



###  http://metarelate.net/config/generatesType

<http://metarelate.net/config/generatesType> rdf:type owl:ObjectProperty ;
                                             
                                             rdfs:comment """<x> generatesType <y> implies that for any satement:
a rdf:type <x>, a new statement a rdf:type <y> is generated.""" ;
                                             
                                             rdfs:subPropertyOf <http://metarelate.net/config/reasonerConfigProperty> .



###  http://metarelate.net/config/hasAuthConfigType

<http://metarelate.net/config/hasAuthConfigType> rdf:type owl:ObjectProperty ;
                                                 
                                                 rdfs:comment "Specifies which kind of authorization system is in place. " ;
                                                 
                                                 rdfs:range <http://metarelate.net/config/AuthorizationSystem> ;
                                                 
                                                 rdfs:subPropertyOf <http://metarelate.net/config/authConfigProperty> .



###  http://metarelate.net/config/hasDefaultUserID

<http://metarelate.net/config/hasDefaultUserID> rdf:type owl:ObjectProperty ;
                                                
                                                rdfs:comment "Links an innstance to its default user." ;
                                                
                                                rdfs:domain <http://metarelate.net/config/Instance> .



###  http://metarelate.net/config/hasEffectOnCode

<http://metarelate.net/config/hasEffectOnCode> rdf:type owl:ObjectProperty ;
                                               
                                               rdfs:comment "Specifies which roles apply to this action. Roles specified via this properties are considered to apply to the code in a register (process requests are defined x register)." ;
                                               
                                               rdfs:domain <http://metarelate.net/config/Action> ;
                                               
                                               rdfs:range <http://metarelate.net/config/Role> ;
                                               
                                               rdfs:subPropertyOf <http://metarelate.net/config/processConfigurationProperty> .



###  http://metarelate.net/config/hasEffectOnReg

<http://metarelate.net/config/hasEffectOnReg> rdf:type owl:ObjectProperty ;
                                              
                                              rdfs:comment "Specifies which roles apply to this action. Roles specified via this properties are considered to apply to the register r (process requests are defined x register, but can apply independently to the register or the contained codes)." ;
                                              
                                              rdfs:subPropertyOf <http://metarelate.net/config/processConfigurationProperty> .



###  http://metarelate.net/config/hasPragma

<http://metarelate.net/config/hasPragma> rdf:type owl:ObjectProperty ;
                                         
                                         rdfs:comment "specifies a prgama operator to be executed during import, for a code or register." ;
                                         
                                         rdfs:subPropertyOf <http://metarelate.net/config/PragmaConfigProperties> .



###  http://metarelate.net/config/hasSymmetric

<http://metarelate.net/config/hasSymmetric> rdf:type owl:ObjectProperty ;
                                            
                                            rdfs:comment "Specifes a symmetric property for the property object of this validation. Currently unimplemeneted, this could be used to automatically generate symmetric relations." ;
                                            
                                            rdfs:subPropertyOf <http://metarelate.net/config/ValidationConfigProperties> ;
                                            
                                            rdfs:domain <http://metarelate.net/config/ValidationTypes> .



###  http://metarelate.net/config/hasType

<http://metarelate.net/config/hasType> rdf:type owl:ObjectProperty ;
                                       
                                       rdfs:comment "Specifies types restrictions for a (data) property." ;
                                       
                                       rdfs:range <http://metarelate.net/config/DataTypes> ;
                                       
                                       rdfs:subPropertyOf <http://metarelate.net/config/ValidationConfigProperties> ;
                                       
                                       rdfs:domain <http://metarelate.net/config/ValidationTypes> .



###  http://metarelate.net/config/inRegister

<http://metarelate.net/config/inRegister> rdf:type owl:ObjectProperty ;
                                          
                                          rdfs:comment "The object of the specified (obejct) property must be contained in the specified register." ;
                                          
                                          rdfs:subPropertyOf <http://metarelate.net/config/ValidationConfigProperties> ;
                                          
                                          rdfs:domain <http://metarelate.net/config/ValidationTypes> .



###  http://metarelate.net/config/onDataProperty

<http://metarelate.net/config/onDataProperty> rdf:type owl:ObjectProperty ;
                                              
                                              rdfs:comment "the validation rules applies to the specified property (which is intended as a data property" ;
                                              
                                              rdfs:subPropertyOf <http://metarelate.net/config/ValidationConfigProperties> .



###  http://metarelate.net/config/onObjectProperty

<http://metarelate.net/config/onObjectProperty> rdf:type owl:ObjectProperty ;
                                                
                                                rdfs:comment "the validation rules applies to the specified property (which is intended as an object property)" ;
                                                
                                                rdfs:subPropertyOf <http://metarelate.net/config/ValidationConfigProperties> ;
                                                
                                                rdfs:domain <http://metarelate.net/config/ValidationTypes> .



###  http://metarelate.net/config/oneOf

<http://metarelate.net/config/oneOf> rdf:type owl:ObjectProperty ;
                                     
                                     rdfs:comment "the object of the (object) property must be one of the elements specified in the oneOf clauses (several can be repeated)." ;
                                     
                                     rdfs:subPropertyOf <http://metarelate.net/config/ValidationConfigProperties> .



###  http://metarelate.net/config/overrideProp

<http://metarelate.net/config/overrideProp> rdf:type owl:ObjectProperty ;
                                            
                                            rdfs:comment """A property to be overridden in a pragma expansion.
E.g.: 
entity/1-10 skos:notation 1-10

if the programa ovverideProp skos:notation,
entities are going to be expanded as follows:
entity/1 skos:notation 1
entity/2 skos:notation 2
...

(instead of, e.g.: entity/1 skos:notation 1-10)

""" ;
                                            
                                            rdfs:domain <http://metarelate.net/config/ExpandDashAndSuppress> ;
                                            
                                            rdfs:subPropertyOf <http://metarelate.net/config/PragmaConfigProperties> .



###  http://metarelate.net/config/postAux

<http://metarelate.net/config/postAux> rdf:type owl:ObjectProperty ;
                                       
                                       rdfs:comment "The resulting state of an auxiliary entitity (e.g.: a superseding term)" ;
                                       
                                       rdfs:domain <http://metarelate.net/config/Role> ;
                                       
                                       rdfs:range <http://metarelate.net/config/State> ;
                                       
                                       rdfs:subPropertyOf <http://metarelate.net/config/processConfigurationProperty> .



###  http://metarelate.net/config/postDown

<http://metarelate.net/config/postDown> rdf:type owl:ObjectProperty ;
                                        
                                        rdfs:comment "The resulting state for the children of the entity (how this varies respect to individual states is undefined)." ;
                                        
                                        rdfs:domain <http://metarelate.net/config/Role> ;
                                        
                                        rdfs:range <http://metarelate.net/config/State> ;
                                        
                                        rdfs:subPropertyOf <http://metarelate.net/config/processConfigurationProperty> .



###  http://metarelate.net/config/postThis

<http://metarelate.net/config/postThis> rdf:type owl:ObjectProperty ;
                                        
                                        rdfs:comment "the resulting state for the entity." ;
                                        
                                        rdfs:domain <http://metarelate.net/config/Role> ;
                                        
                                        rdfs:range <http://metarelate.net/config/State> ;
                                        
                                        rdfs:subPropertyOf <http://metarelate.net/config/processConfigurationProperty> .



###  http://metarelate.net/config/postUp

<http://metarelate.net/config/postUp> rdf:type owl:ObjectProperty ;
                                      
                                      rdfs:comment "The resulting state for the container of the entity" ;
                                      
                                      rdfs:domain <http://metarelate.net/config/Role> ;
                                      
                                      rdfs:range <http://metarelate.net/config/State> ;
                                      
                                      rdfs:subPropertyOf <http://metarelate.net/config/processConfigurationProperty> .



###  http://metarelate.net/config/pragmaProp

<http://metarelate.net/config/pragmaProp> rdf:type owl:ObjectProperty ;
                                          
                                          rdfs:domain <http://metarelate.net/config/ExpandDashAndSuppress> ;
                                          
                                          rdfs:range <http://metarelate.net/config/Parameter> ;
                                          
                                          rdfs:subPropertyOf <http://metarelate.net/config/PragmaConfigProperties> .



###  http://metarelate.net/config/pragmaSchema

<http://metarelate.net/config/pragmaSchema> rdf:type owl:ObjectProperty ;
                                            
                                            rdfs:comment "The schema in which the tree sctructure should be expressed" ;
                                            
                                            rdfs:domain <http://metarelate.net/config/ExpandTreeProcedure> ;
                                            
                                            rdfs:subPropertyOf <http://metarelate.net/config/PragmaConfigProperties> .



###  http://metarelate.net/config/pragmaTreeCollection

<http://metarelate.net/config/pragmaTreeCollection> rdf:type owl:ObjectProperty ;
                                                    
                                                    rdfs:comment "The collection in which new entities should be created." ;
                                                    
                                                    rdfs:domain <http://metarelate.net/config/ExpandTreeProcedure> ;
                                                    
                                                    rdfs:subPropertyOf <http://metarelate.net/config/PragmaConfigProperties> .



###  http://metarelate.net/config/preAux

<http://metarelate.net/config/preAux> rdf:type owl:ObjectProperty ;
                                      
                                      rdfs:comment """Pre condition: the state that an auxiliary entity must be in for this role to be \"triggered\" (e.g.: the state of a superseding term).
Note that all states (preUp, preDown, preThis, preAux) are considered in AND. Non specified states are considered as always viable.""" ;
                                      
                                      rdfs:domain <http://metarelate.net/config/Role> ;
                                      
                                      rdfs:range <http://metarelate.net/config/State> ;
                                      
                                      rdfs:subPropertyOf <http://metarelate.net/config/processConfigurationProperty> .



###  http://metarelate.net/config/preDown

<http://metarelate.net/config/preDown> rdf:type owl:ObjectProperty ;
                                       
                                       rdfs:comment """Pre condition: the state that the children of the entity must be in for this role to be \"triggered\" (only makes sense for registers)
Note that all states (preUp, preDown, preThis, preAux) are considered in AND. Non specified states are considered as always viable.""" ;
                                       
                                       rdfs:domain <http://metarelate.net/config/Role> ;
                                       
                                       rdfs:range <http://metarelate.net/config/State> ;
                                       
                                       rdfs:subPropertyOf <http://metarelate.net/config/processConfigurationProperty> .



###  http://metarelate.net/config/preThis

<http://metarelate.net/config/preThis> rdf:type owl:ObjectProperty ;
                                       
                                       rdfs:comment """Pre condition: the state that the entity must be in for this role to be \"triggered\".
Note that all states (preUp, preDown, preThis, preAux) are considered in AND. Non specified states are considered as always viable.""" ;
                                       
                                       rdfs:domain <http://metarelate.net/config/Role> ;
                                       
                                       rdfs:range <http://metarelate.net/config/State> ;
                                       
                                       rdfs:subPropertyOf <http://metarelate.net/config/processConfigurationProperty> .



###  http://metarelate.net/config/preUp

<http://metarelate.net/config/preUp> rdf:type owl:ObjectProperty ;
                                     
                                     rdfs:comment """Pre condition: the state that the container of the entity must be in for this role to be \"triggered\".
Note that all states (preUp, preDown, preThis, preAux) are considered in AND. Non specified states are considered as always viable.""" ;
                                     
                                     rdfs:domain <http://metarelate.net/config/Role> ;
                                     
                                     rdfs:range <http://metarelate.net/config/State> ;
                                     
                                     rdfs:subPropertyOf <http://metarelate.net/config/processConfigurationProperty> .



###  http://metarelate.net/config/processConfigurationProperty

<http://metarelate.net/config/processConfigurationProperty> rdf:type owl:ObjectProperty ;
                                                            
                                                            rdfs:domain <http://metarelate.net/config/Action> ;
                                                            
                                                            rdfs:range <http://metarelate.net/config/Role> .



###  http://metarelate.net/config/propertyFocus

<http://metarelate.net/config/propertyFocus> rdf:type owl:ObjectProperty ;
                                             
                                             rdfs:comment "Whether a property pertains to a code or to the concept represented," ;
                                             
                                             rdfs:range <http://metarelate.net/config/PropertyFocus> .



###  http://metarelate.net/config/reasonerConfigProperty

<http://metarelate.net/config/reasonerConfigProperty> rdf:type owl:ObjectProperty .



###  http://metarelate.net/config/symmetric

<http://metarelate.net/config/symmetric> rdf:type owl:ObjectProperty ;
                                         
                                         rdfs:comment """<x> symmetrix <y> implies that for any statement:
a <x> b, a new statement b <y> a is generated (and vice versa).""" ;
                                         
                                         rdfs:subPropertyOf <http://metarelate.net/config/reasonerConfigProperty> .



###  http://metarelate.net/config/validationCommand

<http://metarelate.net/config/validationCommand> rdf:type owl:ObjectProperty ;
                                                 
                                                 rdfs:comment "Associated a validation command to a validation rule." ;
                                                 
                                                 rdfs:range <http://metarelate.net/config/ValidationCommands> ;
                                                 
                                                 rdfs:subPropertyOf <http://metarelate.net/config/ValidationConfigProperties> ;
                                                 
                                                 rdfs:domain <http://metarelate.net/config/ValidationTypes> .



###  http://metarelate.net/states/overrides

<http://metarelate.net/states/overrides> rdf:type owl:ObjectProperty ;
                                         
                                         rdfs:comment "Re-defines a system property or resource" .





