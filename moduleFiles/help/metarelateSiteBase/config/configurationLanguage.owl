###  http://metarelate.net/config/baseURL

<http://metarelate.net/config/baseURL> rdf:type owl:DatatypeProperty ;
                                       
                                       rdfs:comment "The root of the server URL (after which the terminology server can append its bits). Note that this is a parameter and hence specified as a literal." ;
                                       
                                       rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/cssAddress

<http://metarelate.net/config/cssAddress> rdf:type owl:DatatypeProperty ;
                                          
                                          rdfs:comment "the location of a css file to be used for rendering (possibly obsolete in the the new rendering system)." ;
                                          
                                          rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/diskPrefix

<http://metarelate.net/config/diskPrefix> rdf:type owl:DatatypeProperty ;
                                          
                                          rdfs:comment "the directory where a file-based Linked-Data representation can be generated." ;
                                          
                                          rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/hardLimit

<http://metarelate.net/config/hardLimit> rdf:type owl:DatatypeProperty ;
                                         
                                         rdfs:comment """Specifies a limit for range expansions: e.g.: entity/1-100.
If an hardLimit of 20 is specified,  only entity/1....entity/20 are produced.
Unless hardLimitCut is specified via pragmaProp, in which case none is produced.""" ;
                                         
                                         rdfs:domain <http://metarelate.net/config/ExpandDashAndSuppress> ;
                                         
                                         rdfs:subPropertyOf <http://metarelate.net/config/pragmaDataProperty> .



###  http://metarelate.net/config/hasAuthDir

<http://metarelate.net/config/hasAuthDir> rdf:type owl:DatatypeProperty ;
                                          
                                          rdfs:comment "Specifies the directory where authorization files can be found (absolute path)"@en ;
                                          
                                          rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/hasCacheDir

<http://metarelate.net/config/hasCacheDir> rdf:type owl:DatatypeProperty ;
                                           
                                           rdfs:comment "Specifies the directory where cache files are found (absolute dirs)"@en ;
                                           
                                           rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/hasDBDir

<http://metarelate.net/config/hasDBDir> rdf:type owl:DatatypeProperty ;
                                        
                                        rdfs:comment "The location of the TDB in use by the system" ;
                                        
                                        rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/hasGitDir

<http://metarelate.net/config/hasGitDir> rdf:type owl:DatatypeProperty ;
                                         
                                         rdfs:comment "Specifies teh directory used for git support to distributed code management system (currently unimplemented)."@en ;
                                         
                                         rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/hasInstanceIdentifier

<http://metarelate.net/config/hasInstanceIdentifier> rdf:type owl:DatatypeProperty ;
                                                     
                                                     rdfs:comment "A unique string identifying an instance (usually a UUID)." ;
                                                     
                                                     rdfs:domain <http://metarelate.net/config/Instance> ;
                                                     
                                                     rdfs:subPropertyOf <http://metarelate.net/config/instanceDataProperties> .



###  http://metarelate.net/config/hasPdfLatexCommand

<http://metarelate.net/config/hasPdfLatexCommand> rdf:type owl:DatatypeProperty ;
                                                  
                                                  rdfs:comment "Speciefies where the pdflatex command can be found (absolute path)"@en ;
                                                  
                                                  rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/hasPrefixFile

<http://metarelate.net/config/hasPrefixFile> rdf:type owl:DatatypeProperty ;
                                             
                                             rdfs:comment "A files whose prefixes are  used in the rdf exports"@en ;
                                             
                                             rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/hasSeedFile

<http://metarelate.net/config/hasSeedFile> rdf:type owl:DatatypeProperty ;
                                           
                                           rdfs:comment "A file containing the identifier (seed) for this instance."@en ;
                                           
                                           rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/hasTemplatesDir

<http://metarelate.net/config/hasTemplatesDir> rdf:type owl:DatatypeProperty ;
                                               
                                               rdfs:comment "Specifies the directory where template files can be found (abosulte files). Files are grouped in sub-directory of the declared directory, named after the template name."@en ;
                                               
                                               rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/hasWebPackageDir

<http://metarelate.net/config/hasWebPackageDir> rdf:type owl:DatatypeProperty ;
                                                
                                                rdfs:comment "the directory where the WAR application for the admin interface can be found."@en ;
                                                
                                                rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/instanceDataProperties

<http://metarelate.net/config/instanceDataProperties> rdf:type owl:DatatypeProperty .



###  http://metarelate.net/config/language

<http://metarelate.net/config/language> rdf:type owl:DatatypeProperty ;
                                        
                                        rdfs:comment """the language for this property. Note that for cardinality counts, a property with a language specified is consider as a distinct property.
E.g.: myLabel with language \"en\" and myLabel withLnaguge \"fr\" are two distinct properties.
Object of thei property should be a two letter language code, as defiend in the RDF specs.""" ;
                                        
                                        rdfs:subPropertyOf <http://metarelate.net/config/validationDataProperties> .



###  http://metarelate.net/config/maxCardinality

<http://metarelate.net/config/maxCardinality> rdf:type owl:DatatypeProperty ;
                                              
                                              rdfs:comment "the maximum number of times this property can be asserted." ;
                                              
                                              rdfs:domain <http://metarelate.net/config/ValidationTypes> ;
                                              
                                              rdfs:subPropertyOf <http://metarelate.net/config/validationDataProperties> .



###  http://metarelate.net/config/minCardinality

<http://metarelate.net/config/minCardinality> rdf:type owl:DatatypeProperty ;
                                              
                                              rdfs:comment "the minimum number of times this property has to be asserted (0=optional)" ;
                                              
                                              rdfs:domain <http://metarelate.net/config/ValidationTypes> ;
                                              
                                              rdfs:subPropertyOf <http://metarelate.net/config/validationDataProperties> .



###  http://metarelate.net/config/overridesDiskPrefix

<http://metarelate.net/config/overridesDiskPrefix> rdf:type owl:DatatypeProperty ;
                                                   
                                                   rdfs:comment "Specifies an alternative directory that, for a specific action, can ovverride the system-wide diretcory used for a file-based linked-data representation. In use a configuration of the publication pipeline, subject to reworking." ;
                                                   
                                                   owl:deprecated "" ;
                                                   
                                                   rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/overridesSitePrefix

<http://metarelate.net/config/overridesSitePrefix> rdf:type owl:DatatypeProperty ;
                                                   
                                                   rdfs:comment "Specifies an alternative baseURL that can ovverride the system-wide baseURL for a specific action. In use a configuration of the publication pipeline, subject to reworking." ;
                                                   
                                                   owl:deprecated "" ;
                                                   
                                                   rdfs:subPropertyOf <http://metarelate.net/config/systemParameters> .



###  http://metarelate.net/config/pattern

<http://metarelate.net/config/pattern> rdf:type owl:DatatypeProperty ;
                                       
                                       rdfs:comment "a Java regex the code or register notation must comply to." ;
                                       
                                       rdfs:subPropertyOf <http://metarelate.net/config/validationDataProperties> .



###  http://metarelate.net/config/pragmaDataProperty

<http://metarelate.net/config/pragmaDataProperty> rdf:type owl:DatatypeProperty .



###  http://metarelate.net/config/pragmaPad

<http://metarelate.net/config/pragmaPad> rdf:type owl:DatatypeProperty ;
                                         
                                         rdfs:comment """Specifies a pad value for index expansions.
E.g.: if entity/1-100 is expanded, and pragmaPad is 5, entities will be:
entity/00001, entity/00002 ...

(note, in some cases, auto padding is in use).""" ;
                                         
                                         rdfs:domain <http://metarelate.net/config/ExpandDashAndSuppress> ;
                                         
                                         rdfs:subPropertyOf <http://metarelate.net/config/pragmaDataProperty> .



###  http://metarelate.net/config/pseudoOrder

<http://metarelate.net/config/pseudoOrder> rdf:type owl:DatatypeProperty ;
                                           
